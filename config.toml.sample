[email]
server   = "imap.zombo.com"
port     = 993
username = "<your-email-address>"
password = "<your-email-password>"
folder   = "Packages"

check_interval_seconds = 30

[courier.fedex]
client_id = "your-fedex-client-id"
client_secret = "your-fedex-client-secret"

[courier.ups]
client_id = "your-ups-client-id"
client_secret = "your-ups-client-secret"

[courier.usps]
client_id = "your-usps-consumer-key"
client_secret = "your-usps-consumer-secret"
